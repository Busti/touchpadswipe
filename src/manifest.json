{
  "content_scripts": [
    {
      "css": [
        "css/anim.css"
      ],
      "js": [
        "jquery.min.js",
        "mousewheel.js",
        "swipe.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "description": "Enables multitouch swipe (backwards/forwards) in chrome as in OSX for windows notebooks with touchpads.",
  "name": "TouchpadSwipe",
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ],
  "version": "1.2.1",
  "manifest_version": 2
}
